{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",

    "no-unused-vars": ["warn", { "argsIgnorePattern": "h|request|_" }],
    "no-undef": "off",

    "linebreak-style": "off",
    "quotes": "off",
    "lines-between-class-members": "off",
    "no-dupe-class-members": "off"
  }
}
